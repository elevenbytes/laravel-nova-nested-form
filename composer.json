{
    "name": "elbytes/nova-nested-form",
    "description": "A Laravel Nova package that allows you to create/update/delete nested related fields from a parent form.",
    "keywords": [
        "laravel",
        "nova",
        "nova 4",
        "nested",
        "relationship",
        "form"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "laravel/nova": "^4.17"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Elbytes\\NestedForm\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Elbytes\\NestedForm\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
